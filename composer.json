{
  "name": "apie/rest-api",
  "description": "Composer package of the apie library: rest api",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\RestApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\RestApi\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "apie/common": "self.version",
    "apie/schema-generator": "self.version",
    "apie/serializer": "self.version",
    "devizzent/cebe-php-openapi": "^1.1.2",
    "nyholm/psr7": "^1.8.2",
    "psr/http-message": "^1.1",
    "symfony/event-dispatcher": "^7.2"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.3",
    "apie/text-value-objects": "self.version",
    "illuminate/support": "*",
    "phpunit/phpunit": "^11.5.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
